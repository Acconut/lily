# Make sure string::format can handle multiple formats by each other.

var s: string = "    "
var result = string::format("%s%s%s%s%s%s%s%s%s%s",
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic,
        s |> Dynamic)

if result != "                                        ":
    stderr.print("Failed.")
