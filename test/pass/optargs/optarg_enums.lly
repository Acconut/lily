enum Option[A] {
    Some(A)
    None
    define unwrap[A]: A
    {
        match self: {
            case Some(s):
                return s
            case None:
                raise ValueError::new("No value!")
        }
    }
}

define my_print(message: string, target: *Option[file] = None)
{
    if target == None:
        target = Some(stdout)

    var file_target = target.unwrap()
}

my_print("Hello")
my_print("Hello", Some(stdout))
