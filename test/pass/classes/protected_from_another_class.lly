class Point(x: integer, y: integer)
{
    protected var @x = x
    protected var @y = y
    protected define f { }
}

class Point3d(x: integer, y: integer, z: integer) < Point(x, y)
{
    f()
    Point::f(self)

    @x += 1
    @y += 1
    var @p = Point::new(1, 2)
    @p.x = 1
    @p.y = 1
    @p.x = @x
    @p.y = @y
}
